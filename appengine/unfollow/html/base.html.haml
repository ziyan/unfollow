{{ define "body" }}
#base.base<>
  .navbar.navbar-default.navbar-fixed-top{role: "navigation"}<>
    .container
      .navbar-header<>
        %button.navbar-toggle{type: "button", "data-toggle" => "collapse", "data-target" => ".navbar-collapse"}<>
          %span.sr-only<> {{ .GetText "Toggle navigation" }}
          %span.icon-bar<>
          %span.icon-bar<>
          %span.icon-bar<>
        %a.navbar-brand{href: "{{ if .Session.User }}{{ url \"page:home\" }}{{ else }}{{ url \"page:welcome\" }}{{ end }}"}<> {{ .GetText "Un<b>follow</b>" | safe }}
      .navbar-collapse.collapse<> {{ template "nav" . }}{{ template "status" . }}
  .content<> {{ template "content" . }}
{{ template "footer" . }}{{ template "settings" . }}{{ end }}
